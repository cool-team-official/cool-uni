<view class="{{['cl-tabs','cl-tabs--'+type,isSticky]}}"><scroll-view class="cl-tabs__bar" style="{{'top:'+(stickyTop)+';'}}" scroll-with-animation scroll-x scroll-left="{{scrollLeft}}"><block wx:for="{{list2}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['change',[index]]]]]}}" class="{{['cl-tabs__bar-li',[(value==item.name)?'active':'']]}}" bindtap="__e"><block wx:if="{{item.prefixIcon}}"><text class="{{['cl-tabs__icon','cl-tabs__icon--prefix',item.prefixIcon]}}"></text></block><text class="cl-tabs__label">{{item.label}}</text><block wx:if="{{item.suffixIcon}}"><text class="{{['cl-tabs__icon','cl-tabs__icon--suffix',item.suffixIcon]}}"></text></block></view></block></scroll-view><view class="cl-tabs__pane"><block wx:if="{{type==='swiper'}}"><swiper class="cl-tabs__swiper" circular current="{{current}}" data-event-opts="{{[['change',[['onChangeSwiper',['$event']]]]]}}" bindchange="__e"><block wx:for="{{list2}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><scroll-view class="{{['cl-tabs__swiper-scroller']}}" scroll-y enable-back-to-top><block wx:if="{{item.visible}}"><scoped-slots-default item="{{item}}" index="{{index}}" bind:__l="__l"></scoped-slots-default></block></scroll-view></swiper-item></block></swiper></block><block wx:else><slot></slot></block></view></view>